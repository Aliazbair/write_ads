---
import { Links } from '../data';
---

<section class='bg-white text-gray-500 text-[12px] py-[35px]'>
  <div class=''>
    <ul
      class='max-w-[1100px] mx-auto px-[20px] grid grid-cols-6 gap-[10px] items-start justify-center'
    >
      {
        Links.map((link) => (
          <li class=''>
            <h3>{link.title}</h3>
             <a href="" class='flex flex-col items-center justify-center '> {link.items}</a>
          </li>
        ))
      }
    </ul>
  </div>
</section>
